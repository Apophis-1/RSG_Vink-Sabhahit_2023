

&star_job

  ! -- begin with pre-main sequence model -- 
    create_pre_main_sequence_model = .true.
    pre_ms_relax_num_steps = 100
    pre_ms_T_c = 3d5


  ! -- rotation --
    new_rotation_flag = .true.
    change_rotation_flag = .false.  ! rotation off unil near zams
    new_omega_div_omega_crit = 0
    near_zams_relax_omega_div_omega_crit = .true.
    num_steps_to_relax_rotation = 50 


  ! -- prevent pgstar closing before terminating --
    pause_before_terminate = .true.


  ! -- save the last pgstar output --
    save_pgstar_files_when_terminate = .true.
    

  ! -- save model after hydrogen burning --



  ! -- report on the retries -- 
    !report_retries = .true.


  ! -- display on-screen plots --
    pgstar_flag = .true.

    
  ! -- opacity table --
    kappa_file_prefix = 'gs98'
    show_kap_info = .true.


  ! -- Z fractions --
    initial_zfracs = 3


  ! -- change net --
    change_net = .true.
    new_net_name = 'basic.net'





/ !end of star_job namelist


&controls

  ! -- starting specifications (mass and metallicity) --
    initial_mass = 35
    initial_z = 0.02
    log_directory ='/Users/gauthamsabhahit/Documents/mesa-r12115/work/RSG_mass_loss_paper/MODELS/LOGS_35_1'


  ! -- opacities (massive star defaults) --
    use_Type2_opacities = .true.
    Zbase = 0.02


  !  -- output options -- 
    history_interval = 5
    profile_interval = 100
    max_num_profile_models = 10000


  ! -- mixing and exponential overshooting parameters --
    mixing_length_alpha = 1.5

    use_ledoux_criterion = .true.
    alpha_semiconvection = 1
    thermohaline_coeff = 0

    step_overshoot_f_above_burn_h_core = 0.1
    overshoot_f0_above_burn_h_core = 0.001

    step_overshoot_f_above_burn_he_core = 0.1
    overshoot_f0_above_burn_he_core = 0.001


    set_min_D_mix = .true.
    min_D_mix = 1d-2
    D_mix_ov_limit = 1d-2


  ! -- mass loss --
    use_other_wind = .true.
    hot_wind_scheme = 'other'
    Dutch_scaling_factor = 1
    hot_wind_full_on_T = 1d0
    cool_wind_full_on_T = 0d0
    mdot_omega_power = 0



  ! -- form of energy equation (MESA paper 4) --
    use_dedt_form_of_energy_eqn = .true.
    include_composition_in_eps_grav = .true.
    convergence_ignore_equL_residuals = .true.
    use_eosDT2 = .true.   
    use_eosELM = .true.


  ! -- use gold tolerances --
    use_gold_tolerances = .true.


  ! -- stopping condition --
    xa_central_lower_limit_species(1) = 'he4'
    xa_central_lower_limit(1) = 0.01
    !max_number_retries = 1000


  ! -- mesh controls --
    mesh_delta_coeff = 1.5
    

  ! -- timestep controls --
