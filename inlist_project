    delta_lg_XH_cntr_min = -4.0d0

    retry_limit = 8



  ! -- hydro info --
    maxT_for_gold_tolerances = 1.5d9
    backup_hold = 2
    retry_hold = 1  ! default 1


  ! -- MLT++ --
    okay_to_reduce_gradT_excess = .false.



   




  
